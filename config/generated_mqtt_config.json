{
  "topics": {
    "iot_data": {
      "subscribe": "/iot-data",
      "publish": {
        "validated": "te/device/iot-data/measurements",
        "alarm": "te/device/iot-data/alarm"
      },
      "batch_size": 20,
      "variables": [
        "id",
        "sensorId",
        "sensorName",
        "sensorDisplayname",
        "active",
        "organizationId",
        "organizationName",
        "suborganizationId",
        "suborganizationName",
        "dateCaptured",
        "yearCaptured",
        "monthCaptured",
        "dayCaptured",
        "dateCreated",
        "yearCreated",
        "monthCreated",
        "dayCreated",
        "temperature",
        "airPressure",
        "humidity",
        "soilMoisture",
        "leafWetness",
        "batteryLevel",
        "status"
      ]
    },
    "air_quality": {
      "subscribe": "/air-quality",
      "publish": {
        "validated": "te/device/air-quality/measurements",
        "alarm": "te/device/air-quality/alarm"
      },
      "batch_size": 25,
      "variables": [
        "co",
        "no2",
        "o3"
      ]
    }
  }
}