{
  "validations": {
    "air_quality_hourly": {
      "stationId": [
        {
          "rule": "expect_column_values_to_not_be_null",
          "params": {
            "column": "stationId"
          },
          "handler": "RaisAlarm"
        }
      ],
      "stationName": [
        {
          "rule": "expect_column_values_to_not_be_null",
          "params": {
            "column": "stationName"
          },
          "handler": "RaisAlarm"
        }
      ],
      "dateFrom": [],
      "dateTo": [],
      "so2": [
        {
          "rule": "expect_column_values_to_be_between",
          "params": {
            "column": "so2",
            "min_value": 10,
            "max_value": 20
          },
          "handler": "SmoothingOutliers"
        }
      ],
      "co": [],
      "o3": [],
      "no2": [],
      "pm10": [],
      "pm25": [],
      "benzene": []
    },
    "iot-data": {
      "temperature": [
        {
          "rule": "expect_column_values_to_be_between",
          "params": {
            "column": "temperature",
            "min_value": 0,
            "max_value": 35,
            "strict_min": true,
            "strict_max": "true"
          },
          "handler": "SmoothingOutliers"
        }
      ],
      "airPressure": [
        {
          "rule": "expect_column_values_to_be_between",
          "params": {
            "column": "airPressure",
            "min_value": 990,
            "max_value": 1010
          },
          "handler": "RaisAlarm"
        }
      ]
    }
  }
}