{
  "validations": {
    "iot_data": {
      "id": [
        {
          "rule": "expect_column_values_to_not_be_null",
          "params": {
            "column": "id"
          },
          "handler": "RaisAlarm"
        }
      ],
      "sensorId": [
        {
          "rule": "expect_column_values_to_not_be_null",
          "params": {
            "column": "sensorId"
          },
          "handler": "RaisAlarm"
        }
      ],
      "sensorName": [
        {
          "rule": "expect_column_values_to_not_be_null",
          "params": {
            "column": "sensorName"
          }
        }
      ],
      "sensorDisplayname": [],
      "active": [],
      "organizationId": [],
      "organizationName": [],
      "suborganizationId": [],
      "suborganizationName": [],
      "dateCaptured": [],
      "yearCaptured": [],
      "monthCaptured": [],
      "dayCaptured": [],
      "dateCreated": [],
      "yearCreated": [],
      "monthCreated": [],
      "dayCreated": [],
      "temperature": [
        {
          "rule": "expect_column_values_to_be_between",
          "params": {
            "column": "temperature",
            "min_value": 0,
            "max_value": 20
          }
        }
      ],
      "airPressure": [],
      "humidity": [
        {
          "rule": "expect_column_values_to_be_between",
          "params": {
            "column": "humidity",
            "min_value": 45,
            "max_value": 90
          }
        }
      ],
      "soilMoisture": [],
      "leafWetness": [],
      "batteryLevel": [],
      "status": []
    },
    "air_quality": {
      "co": [
        {
          "rule": "expect_column_values_to_be_between",
          "params": {
            "column": "co",
            "min_value": 0.2,
            "max_value": 2.3
          },
          "handler": "SmoothingOutliers"
        }
      ],
      "no2": [
        {
          "rule": "expect_column_values_to_be_between",
          "params": {
            "column": "no2",
            "min_value": 2,
            "max_value": 35,
            "strict_min": true,
            "strict_max": false
          },
          "handler": "SmoothingOutliers"
        }
      ],
      "o3": [
        {
          "rule": "expect_column_values_to_not_be_null",
          "params": {
            "column": "o3"
          },
          "handler": "RaisAlarm"
        }
      ]
    }
  }
}